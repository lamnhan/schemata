{
  "name": "@lamnhan/schemata-app",
  "version": "0.0.0",
  "description": "Commonly used data models.",
  "author": "Lam Nhan",
  "homepage": "https://schemata.lamnhan.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:lib1": "ng build schemata && cpy README.md LICENSE dist/schemata",
    "build:lib2": "ng build ngx-schemata && cpy README.md LICENSE dist/ngx-schemata",
    "build:app": "ng build && ayedocs generate",
    "build": "npm run build:app && npm run build:lib1 && npm run build:lib2",
    "deploy:lib1": "cd ./dist/schemata && npm publish && cd ../../",
    "deploy:lib2": "cd ./dist/ngx-schemata && npm publish && cd ../../",
    "deploy:app": "git add . && git commit -m 'deploy:app' && git push",
    "deploy": "npm run deploy:app && npm run deploy:lib1 && npm run deploy:lib2",
    "test": "ng test schemata",
    "lint": "ng lint schemata",
    "e2e": "ng e2e",
    "coverage": "cat ./coverage/*/lcov.info | coveralls",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 2.37"
  },
  "keywords": [
    "data",
    "database",
    "schematic",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lamnhan/schemata.git"
  },
  "bugs": {
    "url": "https://github.com/lamnhan/schemata/issues"
  },
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@lamnhan/nguix-starter": "0.0.7",
    "@lamnhan/ngx-schemata": "0.0.3",
    "@lamnhan/ngx-useful": "0.0.11",
    "@lamnhan/schemata": "0.0.9",
    "@lamnhan/unistylus": "0.0.10",
    "pluralize": "^8.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular/cli": "~12.0.3",
    "@angular/compiler-cli": "~12.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/pluralize": "0.0.29",
    "coveralls": "^3.1.0",
    "cpy-cli": "^3.1.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "typescript": "~4.2.3"
  }
}
